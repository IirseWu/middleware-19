<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>
<!DOCTYPE html>
<html lang="en">
	<head>
	<base href="<%=basePath%>">
	<!-- 下拉框 -->
	<link rel="stylesheet" href="static/ace/css/chosen.css" />
	<!-- jsp文件头和头部 -->
	<%@ include file="../../system/index/top.jsp"%>
	<!-- 日期框 -->
	<link rel="stylesheet" href="static/ace/css/datepicker.css" />
</head>
<body class="no-skin">
<!-- /section:basics/navbar.layout -->
<div class="main-container" id="main-container">
	<!-- /section:basics/sidebar -->
	<div class="main-content">
		<div class="main-content-inner">
			<div class="page-content">
				<div class="row">
					<div class="col-xs-12">
					
					<form action="robotversion/${msg }.do" name="Form" id="Form" method="post">
						<input type="hidden" name="versionid" id="versionid" value="${pd.versionid}"/>
						<div id="zhongxin" style="padding-top: 13px;">
						<table id="table_report" class="table table-striped table-bordered table-hover">
							
								<input type="hidden" name="VERSIONID" id="versionid" value="${pd.VERSIONID}" maxlength="32" placeholder="这里输入备注1" title="备注1" style="width:98%;"/>
							
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">等级:</td>
								<td><input type="number" name="GRADE" id="grade" value="${pd.GRADE}" maxlength="32" placeholder="这里输入备注2" title="备注2" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">版本:</td>
								<td><input type="text" name="NAME" id="name" value="${pd.NAME}" maxlength="15" placeholder="这里输入备注3" title="备注3" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">内容:</td>
								<td><input type="text" name="CONTENT" id="CONTENT" value="${pd.CONTENT}" maxlength="255" placeholder="这里输入备注4" title="备注4" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">价格:</td>
								<td><input type="text" name="PRICE" id="PRICE" value="${pd.PRICE}" maxlength="12" placeholder="这里输入备注5" title="备注5" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">每日聊天上限:</td>
								<td><input type="number" name="STREAM" id="STREAM" value="${pd.STREAM}" maxlength="32" placeholder="这里输入备注6" title="备注6" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">存储总量:</td>
								<td><input type="number" name="STORAGE" id="STORAGE" value="${pd.STORAGE}" maxlength="32" placeholder="这里输入备注7" title="备注7" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">每日增加上限:</td>
								<td><input type="number" name="USENUM" id="USENUM" value="${pd.USENUM}" maxlength="32" placeholder="这里输入备注8" title="备注8" style="width:98%;"/></td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">创建时间:</td>
								<td><input disabled="disabled"  class="span10 date-picker" name="CREATETIME" id="createtime" value="${pd.CREATETIME}" type="text" data-date-format="yyyy-mm-dd" readonly="readonly" placeholder="---" title="备注10" style="width:98%;"/></td>
							</tr>
							<input name="PURE" type="hidden" value="0"/>
							<%-- <tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">纯净模式:</td>
						  		<td style="vertical-align:top;padding-left:2px;">
						  			<c:if test="${pd.PURE==true || empty pd.PURE}">  
										<select name="PURE">
										  <option value ="1">启用</option>
										  <option value ="0">禁用</option>
										</select>
									</c:if> 
									<c:if test="${pd.PURE==false }">  
										<select name="PURE">
										  <option value ="1">启用</option>
										  <option value ="0" selected="selected">禁用</option>
										</select>
									</c:if> 
								 	
								</td>
							</tr> --%>
							
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">小尾巴:</td>
								<td style="vertical-align:top;padding-left:2px;">
									<c:if test="${pd.TAIL==true || empty pd.TAIL}">  
										<select name="TAIL">
										  <option value ="1">启用</option>
										  <option value ="0">禁用</option>
										</select>
									</c:if> 
									<c:if test="${pd.TAIL==false }">  
										<select name="TAIL">
										  <option value ="1">启用</option>
										  <option value ="0" selected="selected">禁用</option>
										</select>
									</c:if>
								</td>
							</tr>
							<tr>
								<td style="width:75px;text-align: right;padding-top: 13px;">自定义回复:</td>
								<td style="vertical-align:top;padding-left:2px;">
									<c:if test="${pd.FILTER==true || empty pd.FILTER}">  
										<select name="FILTER">
										  <option value ="1">启用</option>
										  <option value ="0">禁用</option>
										</select>
									</c:if> 
									<c:if test="${pd.FILTER==false}">  
										<select name="FILTER">
										  <option value ="1">启用</option>
										  <option value ="0" selected="selected">禁用</option>
										</select>
									</c:if>
								</td>
							</tr>

							<tr>
								<td style="text-align: center;" colspan="10">
									<a class="btn btn-mini btn-primary" onclick="save();">保存</a>
									<a class="btn btn-mini btn-danger" onclick="top.Dialog.close();">取消</a>
								</td>
							</tr>
						</table>
						</div>
						<div id="zhongxin2" class="center" style="display:none"><br/><br/><br/><br/><br/><img src="static/images/jiazai.gif" /><br/><h4 class="lighter block green">提交中...</h4></div>
					</form>
					</div>
					<!-- /.col -->
				</div>
				<!-- /.row -->
			</div>
			<!-- /.page-content -->
		</div>
	</div>
	<!-- /.main-content -->
</div>
<!-- /.main-container -->


	<!-- 页面底部js¨ -->
	<%@ include file="../../system/index/foot.jsp"%>
	<!-- 下拉框 -->
	<script src="static/ace/js/chosen.jquery.js"></script>
	<!-- 日期框 -->
	<script src="static/ace/js/date-time/bootstrap-datepicker.js"></script>
	<!--提示框-->
	<script type="text/javascript" src="static/js/jquery.tips.js"></script>
		<script type="text/javascript">
		$(top.hangge());
		//保存
		function save(){
			if($("#VERSIONID").val()==""){
				$("#VERSIONID").tips({
					side:3,
		            msg:'请输入备注1',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#VERSIONID").focus();
			return false;
			}
			if($("#GRADE").val()==""){
				$("#GRADE").tips({
					side:3,
		            msg:'请输入备注2',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#GRADE").focus();
			return false;
			}
			if($("#NAME").val()==""){
				$("#NAME").tips({
					side:3,
		            msg:'请输入备注3',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#NAME").focus();
			return false;
			}
			if($("#CONTENT").val()==""){
				$("#CONTENT").tips({
					side:3,
		            msg:'请输入备注4',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#CONTENT").focus();
			return false;
			}
			if($("#PRICE").val()==""){
				$("#PRICE").tips({
					side:3,
		            msg:'请输入备注5',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#PRICE").focus();
			return false;
			}
			if($("#STREAM").val()==""){
				$("#STREAM").tips({
					side:3,
		            msg:'请输入备注6',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#STREAM").focus();
			return false;
			}
			if($("#STORAGE").val()==""){
				$("#STORAGE").tips({
					side:3,
		            msg:'请输入备注7',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#STORAGE").focus();
			return false;
			}
			if($("#USENUM").val()==""){
				$("#USENUM").tips({
					side:3,
		            msg:'请输入备注8',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#USENUM").focus();
			return false;
			}
			if($("#PURE").val()==""){
				$("#PURE").tips({
					side:3,
		            msg:'请输入备注9',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#PURE").focus();
			return false;
			}
			if($("#CREATETIME").val()==""){
				$("#CREATETIME").tips({
					side:3,
		            msg:'请输入备注10',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#CREATETIME").focus();
			return false;
			}
			if($("#TAIL").val()==""){
				$("#TAIL").tips({
					side:3,
		            msg:'请输入备注11',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#TAIL").focus();
			return false;
			}
			if($("#FILTER").val()==""){
				$("#FILTER").tips({
					side:3,
		            msg:'请输入备注12',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#FILTER").focus();
			return false;
			}
			if($("#REMAIN1").val()==""){
				$("#REMAIN1").tips({
					side:3,
		            msg:'请输入备注13',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#REMAIN1").focus();
			return false;
			}
			if($("#REMAIN2").val()==""){
				$("#REMAIN2").tips({
					side:3,
		            msg:'请输入备注14',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#REMAIN2").focus();
			return false;
			}
			if($("#REMAIN3").val()==""){
				$("#REMAIN3").tips({
					side:3,
		            msg:'请输入备注15',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#REMAIN3").focus();
			return false;
			}
			if($("#REMAIN4").val()==""){
				$("#REMAIN4").tips({
					side:3,
		            msg:'请输入备注16',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#REMAIN4").focus();
			return false;
			}
			if($("#ICON").val()==""){
				$("#ICON").tips({
					side:3,
		            msg:'请输入备注17',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#ICON").focus();
			return false;
			}
			if($("#MONTHPRICE").val()==""){
				$("#MONTHPRICE").tips({
					side:3,
		            msg:'请输入备注18',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#MONTHPRICE").focus();
			return false;
			}
			if($("#PRICE").val()<0){
				$("#PRICE").tips({
					side:3,
		            msg:'价格不为负数',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#PRICE").focus();
			return false;
			}
			if($("#STREAM").val()<0){
				$("#STREAM").tips({
					side:3,
		            msg:'每日聊天上限不为负数',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#STREAM").focus();
			return false;
			}
			if($("#STORAGE").val()<0){
				$("#STORAGE").tips({
					side:3,
		            msg:'每日存储上限不为负数',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#STORAGE").focus();
			return false;
			}
			if($("#USENUM").val()<0){
				$("#USENUM").tips({
					side:3,
		            msg:'使用数量不为负数',
		            bg:'#AE81FF',
		            time:2
		        });
				$("#USENUM").focus();
				return false;
				}
			$("#Form").submit();
			$("#zhongxin").hide();
			$("#zhongxin2").show();
		}
		
		$(function() {
			//日期框
			$('.date-picker').datepicker({autoclose: true,todayHighlight: true});
		});
		</script>
</body>
</html>